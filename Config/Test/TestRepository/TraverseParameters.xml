			<MemeProperty name="z" value="1"/>
		</Meme>	
		<Meme id="C" metameme="Base">
			<MemeProperty name="x" value="1"/>
		</Meme>	
		<Meme id="D" metameme="Base">
			<MemeProperty name="x" value="3"/>
		</Meme>	
				
		<Meme id="A" metameme="Base">
			<MemeProperty name="x" value="1"/>
			<MemeProperty name="y" value="2"/>
			<MemeProperty name="z" value="3"/>
		</Meme>	
	</ListOfMeme>	
</RMLModule>


